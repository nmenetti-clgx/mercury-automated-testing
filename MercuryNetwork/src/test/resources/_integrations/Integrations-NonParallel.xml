<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Integrations" verbose="2">

  <listeners>
      <listener class-name="utils.ThreadCountChanger"/>
  </listeners>

 <test name="Integrations-NonParallel" >
	
  <groups>
   <run>
    <exclude name="broken" />
   </run>
  </groups>
  
  <packages>
   <package name ="integrations.*"/>
  </packages>

 </test>

</suite>
