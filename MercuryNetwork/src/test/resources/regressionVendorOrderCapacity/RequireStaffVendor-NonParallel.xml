<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Vendor_Order_Capacity_Regression" verbose="2">

  <listeners>
      <listener class-name="utils.ThreadCountChanger"/>
  </listeners>

 <test name="RequireStaffVendor" >

  <groups>
   <run>
    <include name="negative" />
    <include name="positive" />
    <include name="baseline" />
    <include name="newOrder" />
    <exclude name="broken" />
   </run>
  </groups>

  <classes>
   <class name ="regressionVendorOrderCapacity.RequireStaffVendor"/>
  </classes>

 </test>

</suite>
