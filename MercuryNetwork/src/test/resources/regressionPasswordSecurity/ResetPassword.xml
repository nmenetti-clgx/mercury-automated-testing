<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Password_Security_Regression" verbose="2" parallel="methods">

  <listeners>
      <listener class-name="utils.ThreadCountChanger"/>
  </listeners>

 <test name="ResetPassword" >

  <groups>
   <run>
    <exclude name="broken" />
   </run>
  </groups>

  <classes>
   <class name ="regressionPasswordSecurity.ResetPassword"/>
  </classes>

 </test>

</suite>
