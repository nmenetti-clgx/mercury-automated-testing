<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Round_Trip_Matrix" verbose="2">

  <listeners>
      <listener class-name="utils.ThreadCountChanger"/>
  </listeners>

 <test name="DeclineAndAccept-NonParallel" >
	
  <groups>
   <run>
    <exclude name="broken" />
   </run>
  </groups>

  <classes>
   <class name ="matrixRoundTrip.DeclineAndAccept"/>
  </classes>

 </test>

</suite>
