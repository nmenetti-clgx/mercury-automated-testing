<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Systems_Check" verbose="2"  parallel="methods">

  <listeners>
      <listener class-name="utils.ThreadCountChanger"/>
  </listeners>

 <test name="SystemsCheck" >

  <groups>
   <run>
    <exclude name="broken" />
   </run>
  </groups>

  <classes>
   <class name ="systemsCheck.SystemsCheck"/>
  </classes>

 </test>

</suite>
