<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Opened_By_Regression" verbose="2">

  <listeners>
      <listener class-name="utils.ThreadCountChanger"/>
  </listeners>

 <test name="regressionOpenedBy-NonParallel" >

  <groups>
   <run>
    <exclude name="broken" />
   </run>
  </groups>

  <packages>
   <package name ="regressionOpenedBy.*"/>
  </packages>

 </test>

</suite>
